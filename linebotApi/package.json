{
  "name": "irukara-dev",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "dev": "nest build --webpack && npx serverless offline --httpPort 6000 --aws-profile irukara-dev --stage local",
    "develop": "nest build --webpack && npx serverless offline --httpPort 6000 --stage dev",
    "stg": "nest build --webpack && npx serverless offline --httpPort 6000 --stage stg",
    "prd": "nest build --webpack && npx serverless offline --httpPort 6000 --stage prd",
    "offline": "npx serverless offline --httpPort 6000",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --config ./test/unit/jest-unit.js",
    "test:e2e": "jest --config ./test/e2e/jest-unit.json",
    "url": "npx lt --port 6000 --subdomain dev-irukara-chat-bot-by-yozakura --print-requests",
    "urls": "nest build --webpack && ngrok http 6000",
    "deploy:dev": "nest build --webpack && serverless deploy --stage dev --aws-profile irukara-dev --verbose",
    "deploy:stg": "nest build --webpack && serverless deploy --stage stg --aws-profile irukara-dev --verbose",
    "deploy:prd": "nest build --webpack && serverless deploy --stage prd --aws-profile irukara-dev --verbose",
    "cfn-lint": "cfn-lint serverless.yml",
    "sls-remove-deploy-dev": "serverless remove --verbose -s dev --aws-profile irukara-dev",
    "sls-remove-deploy-stg": "serverless remove --verbose -s stg --aws-profile irukara-dev"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.303.0",
    "@aws-sdk/client-eventbridge": "^3.370.0",
    "@aws-sdk/client-scheduler": "^3.370.0",
    "@aws-sdk/lib-dynamodb": "^3.303.0",
    "@aws-sdk/util-dynamodb": "^3.303.0",
    "@line/bot-sdk": "^7.5.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@types/node-fetch": "^2.6.4",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "loadash": "^1.0.0",
    "localtunnel": "^2.0.2",
    "node-fetch": "^3.3.1",
    "openai": "^3.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
    "webpack": "^5.76.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.3.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-layers": "^2.6.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "node": "16.13.2",
    "npm": "9.6.0"
  }
}
